.container
  .panel.panel-default
    %h1.panel-heading= "Hi #{current_user.first_name}! Here are the booking requests for your listings:"
    .panel-body
      %table.table.table-striped
        %thead
          %tr
            %th Listing
            %th Dates
            %th Number of Nights
            %th
            %th
        %tbody
          -@listing_requests.each do |i|
            %tr
              %td="#{i.title}"
